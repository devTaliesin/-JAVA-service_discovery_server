server:
  port: 8761

eureka:
  client:
    registerWithEureka: false 
    fetchRegistry: false
  server:
    enableSelfPreservation: false #dev
#		enableSelfPreservation: false #prod

management:
  endpoints:
    web:
      exposure:
        include: "*"  # 모든 Actuator 엔드포인트를 노출, 운영 환경에서는 필요한 엔드포인트만 노출하는 것이 좋습니다
  endpoint:
    health:
      show-details: "always"  # Health 엔드포인트에서 세부 정보 표시


